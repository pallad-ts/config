{
  "name": "@pallad/config-cli",
  "version": "3.0.0",
  "description": "CLI command that allows to display and verify current configuration",
  "scripts": {
    "compile": "../../node_modules/.bin/pallad-scripts compile",
    "lint": "../../node_modules/.bin/pallad-scripts lint",
    "prepublishOnly": "npm run compile"
  },
  "bin": {
    "pallad-config": "./bin/run"
  },
  "publishConfig": {
    "access": "public"
  },
  "types": "compiled/index.d.ts",
  "main": "compiled/index.js",
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@sweet-monads/either": "^3",
    "chalk": "^5",
    "cosmiconfig": "^8",
    "object-path": "^0.11.8",
    "predicates": "^2.0.3",
    "pretty-format": "^29",
    "tslib": "^2"
  },
  "peerDependencies": {
    "@pallad/config": "*",
    "@pallad/secret": "^1"
  },
  "devDependencies": {
    "@pallad/config": "^3.0.0",
    "@pallad/secret": "^1"
  },
  "oclif": {
    "bin": "pallad-config"
  },
  "files": [
    "compiled",
    "bin",
    "docs",
    "assets"
  ],
  "pallad-config": {
    "file": "./test/testConfig.js"
  },
  "keywords": [
    "config",
    "cli",
    "secret",
    "verify",
    "pallad"
  ],
  "author": "Łukasz Kużyński <lukasz.kuzynski@gmail.com> (http://wookieb.pl)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/pallad-ts/config.git",
    "directory": "packages/cli"
  }
}
