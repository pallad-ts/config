{
  "name": "@pallad/config-cli",
  "version": "1.1.1",
  "description": "CLI command that allows to verify current configuration",
  "scripts": {
    "compile": "../../node_modules/.bin/pallad-scripts compile",
    "lint": "../../node_modules/.bin/pallad-scripts lint",
    "prepublishOnly": "npm run compile"
  },
  "bin": {
    "pallad-config": "./bin/run"
  },
  "publishConfig": {
    "access": "public"
  },
  "types": "compiled/index.d.ts",
  "main": "compiled/index.js",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "chalk": "^4",
    "monet": "^0.9.2",
    "object-path": "^0.11.8",
    "predicates": "^2.0.3",
    "pretty-format": "^27.3.1"
  },
  "peerDependencies": {
    "@pallad/config": "*",
    "@pallad/secret": "^1"
  },
  "devDependencies": {
    "@pallad/config": "*",
    "@pallad/secret": "^1"
  },
  "oclif": {
    "bin": "pallad-config"
  },
  "files": [
    "compiled",
    "bin",
    "docs",
    "assets"
  ],
  "pallad-config": {
    "file": "./test/testConfig.js"
  },
  "keywords": [
    "config",
    "cli",
    "secret",
    "verify",
    "pallad"
  ],
  "author": "Łukasz Kużyński <lukasz.kuzynski@gmail.com> (http://wookieb.pl)",
  "license": "MIT"
}
