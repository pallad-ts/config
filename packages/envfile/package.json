  },
  "license": "MIT",
  "peerDependencies": {
    "@pallad/config": "*"
  },
  "devDependencies": {
    "@pallad/config": "^4.0.0"
  },
  "dependencies": {
    "@pallad/config-common": "^4.0.0",
    "@pallad/errors": "^1.0.1",
    "@sweet-monads/either": "^3",
